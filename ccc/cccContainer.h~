#ifndef _CCCCONTENT_
#define _CCCCONTENT_

#include "cccVersion.h"

class cccContent
{
	public:
		cccContent() {};
		cccContent(cccContent &content) { m_Content = content; };
		CCCCONTENT getContent() { return m_Content; };
		
	private:
		CCCCONTENT m_Content;
};

#endif
